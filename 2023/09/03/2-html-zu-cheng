    <div class="pjax">
        <article id="post-2-html组成" class="article article-type-post" itemscope itemprop="blogPost">
    
        <h1 class="article-title">2-html组成</h1>
    
    <div class="article-meta">
        
        
        
        <span class="book">
            <i class="iconfont icon-category"></i>
            
            
            <a  data-rel="Html">Html</a>
            
        </span>
        
        
        <span class="tag">
            <i class="iconfont icon-tag"></i>
            
            <a class="color5">HTML</a>
            
        </span>
        
    </div>
    <div class="article-meta">
        
            发布时间 : <time class="date" title='最后更新: 2023-09-03 16:44:58'>2023-09-03 16:13</time>
        
    </div>
    <div class="article-meta">
        
        
        <span id="busuanzi_container_page_pv">
            阅读 :<span id="busuanzi_value_page_pv">
                <span class="count-comment">
                    <span class="spinner">
                      <div class="cube1"></div>
                      <div class="cube2"></div>
                    </span>
                </span>
            </span>
        </span>
        
        
    </div>
    
    <div class="toc-ref">
    
        <ol class="toc"><li class="toc-item toc-level-1"><a class="toc-link" href="#HTML%E7%BB%84%E6%88%90"><span class="toc-text">HTML组成</span></a></li></ol>
    
<style>
    .left-col .switch-btn,
    .left-col .switch-area {
